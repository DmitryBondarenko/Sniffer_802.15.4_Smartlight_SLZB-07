            "command": "D:/prog/GnuWin32/bin/make.exe build -f Sources/Target/Sonoff_USB_Dongle_Plus_E/makefile -j8",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "Clean Sonoff Plus E",
            "type": "shell",
            "command": "D:/prog/GnuWin32/bin/make.exe clean -f Sources/Target/Sonoff_USB_Dongle_Plus_E/makefile -j8",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "all Sonoff Plus E",
            "type": "shell",
            "command": "D:/prog/GnuWin32/bin/make.exe rebuild -f ./Sources/Target/Sonoff_USB_Dongle_Plus_E/makefile -j8",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
